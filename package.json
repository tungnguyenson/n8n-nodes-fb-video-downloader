{
	"name": "n8n-nodes-fb-video-downloader",
	"version": "0.1.0",
	"description": "n8n node to get youtube video transcript using yt-transcript package",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://n8n.io",
	"author": {
		"name": "TungNS",
		"email": "nstung@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:tungnguyenson/n8n-nodes-fb-video-downloader.git"
	},
	"main": "index.js",
	"scripts": {
		"preinstall": "",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [],
		"nodes": [
			"dist/nodes/FbVideoDownloader.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"es5-ext",
			"eslint-plugin-n8n-nodes-base",
			"fsevents"
		]
	},
	"dependencies": {
		"@xaviabot/fb-downloader": "^1.0.14"
	}
}
